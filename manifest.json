{
  "manifest_version": 2,
  "name": "Hexxium Dev",
  "version": "1.0",
  "description": "A browser plugin to avoid scammer websites",
  "browser_action": {
	"default_icon": "20216216.png",
	"default_popup": "popup.html"
  },
   "content_scripts": [{
   "matches": ["<all_urls>"],
   "js": ["listener.js"]
   }],
    "permissions": [
    "activeTab",
    "webRequest"
  ]
}